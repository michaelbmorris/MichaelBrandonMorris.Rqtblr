@model User

    @{
        
    }

<h4>Player Details</h4>

<dl class="dl-horizontal">
    <dt>
        First Name
    </dt>
    <dd>
        @Model.FirstName
    </dd>
    <dt>
        Last Name
    </dt>
    <dd>
        @Model.LastName
    </dd>
</dl>

<div class="row">
    <div class="col-sm-6">
        @foreach (var teamPlayer in Model.TeamPlayers)
        {
            <p>
                @teamPlayer.Team.Id
            </p>
        }
    </div>
</div>
